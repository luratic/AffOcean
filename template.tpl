const injectScript = require('injectScript');
const makeString = require('makeString');
const encodeUriComponent = require('encodeUriComponent');
const customerId = makeString(data.customerId);
const transactionId = makeString(data.transactionId);
const amount = makeString(data.amount);
const trackingUrl = 'https://ad.afftrck.com/' + encodeUriComponent(customerId) + '?adv_sub=' + encodeUriComponent(transactionId) + '&amount=' + encodeUriComponent(amount);

injectScript(trackingUrl,data.gtmOnSuccess,data.gtmOnFailure,'affOceanPx');


